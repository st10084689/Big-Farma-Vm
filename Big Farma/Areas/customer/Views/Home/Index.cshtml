@{
    ViewData["Title"] = "Home Page";
}


<!-- Slideshow container -->
<div id="myCarousel" class="carousel slide" data-bs-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-bs-target="#myCarousel" data-bs-slide-to="0" class="active"></li>
        <li data-bs-target="#myCarousel" data-bs-slide-to="1"></li>
        <li data-bs-target="#myCarousel" data-bs-slide-to="2"></li>
    </ol>

    <!-- Slides -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/carousel/tractor.png" alt="First Image" class="d-block w-100" >
            <div class="carousel-caption">
                <h5>Welcome to Figma</h5>
                <p>Discover the power of collaborative design.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel/farmer.png" alt="Second Image" class="d-block w-100">
            <div class="carousel-caption text-start">
                <p>Welcome, Insert your stock so you can sell website.</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/images/carousel/oranges.png" alt="Third Image" class="d-block w-100">
        </div>
    </div>

    <!-- Navigation arrows -->
    <a class="carousel-control-prev" href="#myCarousel" role="button" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </a>
    <a class="carousel-control-next" href="#myCarousel" role="button" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </a>



<!-- Bootstrap JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.7.2/dist/js/bootstrap.bundle.min.js"></script>
